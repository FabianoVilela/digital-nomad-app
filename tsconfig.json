{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/app/*": ["./app/*"],
      "@/assets/*": ["./assets/*"],
      "@/domain/*": ["./src/domain/*"],
      "@/infra/*": ["./src/infra/*"],
      "@/ui/*": ["./src/ui/*"],
      "@/shared/*": ["./src/shared/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
